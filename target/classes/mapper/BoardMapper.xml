<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinstar.mapper.BoardMapper">
 <select id="getBoardList" parameterType="String" resultType="hashmap">
 	select * from mails order by no desc
 </select>
 
 <select id="getBoardDetail" parameterType="String" resultType="hashmap">
 	select * from mails where no = #{no}
 </select>
</mapper>